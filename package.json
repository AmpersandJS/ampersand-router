        "ampersand-version": "^1.0.0"
    },
    "devDependencies": {
        "browserify": "^4.1.10",
        "jshint": "^2.5.3",
        "phantomjs": "^1.9.7-15",
        "precommit-hook": "^1.0.7",
        "run-browser": "^1.3.1",
        "tap-spec": "^0.2.0",
        "tape": "^2.13.2",
        "tape-run": "^0.1.1"
    },
    "homepage": "https://github.com/ampersandjs/ampersand-router",
    "keywords": [
        "clientside",
        "router",
        "history"
    ],
    "license": "MIT",
    "main": "ampersand-router.js",
    "repository": {
